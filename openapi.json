    "/api/ebay/listings/auction": {
      "post": {
        "summary": "Create a new auction listing",
        "tags": [
          "Listings"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 80
                  },
                  "description": {
                    "type": "string",
                    "minLength": 1
                  },
                  "conditionId": {
                    "type": "number"
                  },
                  "buyItNowPrice": {
                    "type": "number"
                  },
                  "categoryId": {
                    "type": "string"
                  },
                  "startPrice": {
                    "type": "number",
                    "exclusiveMinimum": 0
                  },
                  "listingDuration": {
                    "type": "string",
                    "default": "Days_7"
                  },
                  "shippingCost": {
                    "type": "number",
                    "minimum": 0
                  },
                  "postalCode": {
                    "type": "string"
                  },
                  "dispatchTimeMax": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                  },
                  "images": {
                    "type": "object",
                    "properties": {
                      "base64": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      },
                      "urls": {
                        "type": "array",
                        "items": {
                          "type": "string",
                          "format": "uri"
                        }
                      }
                    }
                  }
                },
                "required": [
                  "title",
                  "description",
                  "categoryId",
                  "startPrice",
                  "shippingCost",
                  "postalCode",
                  "dispatchTimeMax"
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Auction listing created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": [
                        "success"
                      ]
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "itemId": {
                          "type": "string"
                        },
                        "title": {
                          "type": "string"
                        },
                        "startTime": {
                          "type": "string"
                        },
                        "endTime": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "itemId",
                        "title",
                        "startTime",
                        "endTime"
                      ]
                    }
                  },
                  "required": [
                    "status",
                    "data"
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": [
                        "error"
                      ]
                    },
                    "message": {
                      "type": "string"
                    },
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "status",
                    "message"
                  ]
                }
              }
            }
          }
        },
        "operationId": "post_api_ebay_listings_auction",
        "x-openai-isConsequential": false
      }
